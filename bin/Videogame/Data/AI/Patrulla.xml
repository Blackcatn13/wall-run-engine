<fsm name="Patrulla">
	<initialState name="Parado"/>
	<state name="Parado">
		<onEnter lua_funtion="on_enter_patrulla_parado()"/>
		<onExit lua_funtion="on_exit_patrulla_parado()"/>
		<Update time=".01" lua_funtion="on_update_patrulla_parado"/>
	</state>
	<state name="Andar_WP">
		<onEnter lua_funtion="on_enter_patrulla_andar_wp()"/>
		<onExit lua_funtion="on_exit_patrulla_andar_wp()"/>
		<Update time=".01" lua_funtion="on_update_patrulla_andar_wp"/>
	</state>
	<state name="Buscar_next_WP">
		<onEnter lua_funtion="on_enter_patrulla_buscar_next_wp()"/>
		<onExit lua_funtion="on_exit_patrulla_buscar_next_wp()"/>
		<Update time=".01" lua_funtion="on_update_patrulla_buscar_next_wp"/>
	</state>
	<state name="Perseguir_Player">
		<onEnter lua_funtion="on_enter_patrulla_perseguir_player()"/>
		<onExit lua_funtion="on_exit_patrulla_perseguir_player()"/>
		<Update time=".01" lua_funtion="on_update_patrulla_perseguir_player"/>
	</state>
</fsm>