<?xml version="1.0" encoding="ISO-8859-1"?>
<Windows OnLoadWindows="CCoreLuaWrapper().m_CoreInstance:getWWSoundManager():PlayEvent('_Success','Music');"
         OnSaveWindows=""
         OnUpdateWindows="">
		  
  <Image name="ControlsGUIBack" posx="0.00" posy="0.00" height="100" width="100" visible="true" active="true"
         default="ControlsBase" OnLoadValue="" OnSaveValue="" flip="" backGround="false">
    <texture name="ControlsBase" name_texture="./Data/GUI/GoPixelGo/EmptyBackground.jpg"/>
  </Image>

 <Image name="Succeed" posx="0.00" posy="0.00" height="100" width="100" visible="true" active="true"
         default="Thanks" OnLoadValue="" OnSaveValue="" flip="" backGround="false">
	<texture name="Thanks" name_texture="./Data/GUI/GoPixelGo/Thanks for playing.tga"/>
  </Image>
 

  <ButtonList name="FinishButtonList" posx="0" posy="0" height="1" width="1" visible="true" active="true">
    <Buttons>
      <row>
      	<Button name="FinishButton" posx="2" posy="78" height="13.00" width="20.00" visible="true" 
                active="true" texture_normal="./Data/GUI/GoPixelGo/ButtonBack_03.tga"
                  texture_over="./Data/GUI/GoPixelGo/ButtonBack_02.tga"
                  texture_clicked="./Data/GUI/GoPixelGo/ButtonBack_01.tga"
                  texture_deactivated="./Data/GUI/GoPixelGo/ButtonBack_03.tga"
                OnClickedAction=" CCoreLuaWrapper().m_CoreInstance:getWWSoundManager():PlayEvent('MenuOver','Music');
  								reset_game();
  								gui_manager:set_is_gameover(false);
  								gui_manager:set_visible_mouse(true);
  								CCoreLuaWrapper().m_CoreInstance:get_graphics_manager():set_is_GUI_displayed(true);
  								CCoreLuaWrapper().m_CoreInstance:getGUIManager():pop_windows();
								CCoreLuaWrapper().m_CoreInstance:getGUIManager():pop_windows();"
                OnOverAction=""/>
      </row>
    </Buttons>
  </ButtonList>
	
</Windows>
